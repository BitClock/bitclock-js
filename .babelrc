{
	presets: [
		['platform', {
			browser: {
				presets: [
					['env', {
						targets: {
							browsers: ['last 2 versions', '> 5%']
						}
					}]
				],
				plugins: [
					'transform-object-assign'
				]
			},
			node: {
				presets: [
					['env', {
						targets: { node: 4 }
					}]
				]
			},
			'*': {
				presets: [
					['env', { useBuiltins: true }]
				]
			}
		}]
	],
	plugins: [
		'transform-object-rest-spread',
		'transform-export-extensions',
		'transform-class-properties'
	],
	env: {
		test: {
			plugins: [
				'istanbul'
			]
		}
	}
}
