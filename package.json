{
  "name": "bitclock",
  "version": "0.1.0-beta.5",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/BitClock/bitclock-js.git"
  },
  "author": {
    "name": "Bernard McManus",
    "email": "bernard.mcmanus.iv@gmail.com",
    "url": "http://github.com/BitClock"
  },
  "engines": {
    "node": ">=4"
  },
  "main": "build",
  "scripts": {
    "build": "npm run build:babel && npm run build:webpack",
    "build:babel": "babel lib --out-dir build",
    "build:clean": "rimraf ./build/*",
    "build:webpack": "webpack",
    "lint": "eslint . --color --ignore-path .gitignore",
    "lint:staged": "lint-staged",
    "prebuild": "npm run build:clean && npm run test",
    "prepublish": "npm run build",
    "pretest": "npm run lint",
    "test": "NODE_ENV=development mocha --colors --compilers js:babel-register test"
  },
  "lint-staged": {
    "*.js": "lint"
  },
  "pre-commit": "lint:staged",
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.1.3",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "bluebird": "^3.4.7",
    "body-parser": "^1.16.1",
    "chai": "^3.5.0",
    "connect-timeout": "^1.8.0",
    "eslint": "^3.5.0",
    "express": "^4.14.1",
    "intercept-stdout": "^0.1.2",
    "json-loader": "^0.5.4",
    "lint-staged": "^3.0.2",
    "lodash": "^4.17.4",
    "mocha": "^3.2.0",
    "nodemon": "^1.9.2",
    "null-loader": "^0.1.1",
    "pre-commit": "^1.1.3",
    "rimraf": "^2.6.0",
    "strip-ansi": "^3.0.1",
    "webpack": "^2.2.1"
  }
}
